{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "cleo-dash",
	"main": "src/server/index.ts",
	"compatibility_date": "2025-03-27",
	"compatibility_flags": [
		"nodejs_compat",
		"nodejs_als",
		"no_handle_cross_request_promise_resolution"
	],
	"routes": [
		{
			"pattern": "api.cleoai.cloud",
			"custom_domain": true
		}
	],
	"workers_dev": false,
	"preview_urls": false,
	"observability": {
		"enabled": true
	},
	"placement": {
		"mode": "smart"
	},
	"dev": {
		"port": 8080
	}
}
